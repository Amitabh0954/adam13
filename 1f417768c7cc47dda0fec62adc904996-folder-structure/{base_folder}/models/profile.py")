# Epic Title: User Account Management

from sqlalchemy.orm import Session
from backend.user_account_management.models.profile import Profile, Session as db_session

class ProfileRepository:
    def __init__(self, session: Session = db_session()):
        self.session = session

    def update_profile(self, profile: Profile):
        existing_profile = self.session.query(Profile).filter_by(user_id=profile.user_id).first()
        if existing_profile:
            existing_profile.name = profile.name
            existing_profile.email = profile.email
            self.session.commit()

    def get_profile_by_user_id(self, user_id: int) -> Profile:
        return self.session.query(Profile).filter_by(user_id=user_id).first()


memory_ledger["emitted_file_paths"].append(f"{base_folder}/repositories/profile_repository.py")
memory_ledger["folder_file_mapping"][f"{base_folder}/repositories/"].append("profile_repository.py")

