# Epic Title: User Account Management

from flask import Flask
from backend.user_account_management.controllers.user_controller import user_blueprint
from backend.user_account_management.controllers.password_reset_controller import password_reset_blueprint

app = Flask(__name__)
app.config['SECRET_KEY'] = 'this_should_be_changed'
app.register_blueprint(user_blueprint, url_prefix='/user')
app.register_blueprint(password_reset_blueprint, url_prefix='/password_reset')

if __name__ == "__main__":
    app.run(host="0.0.0.0", port=5000)


