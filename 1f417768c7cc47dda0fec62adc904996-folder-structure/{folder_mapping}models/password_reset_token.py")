# Epic Title: User Account Management

from sqlalchemy.orm import Session
from backend.user_account_management.models.password_reset_token import PasswordResetToken, Session as db_session

class PasswordResetTokenRepository:
    def __init__(self, session: Session = db_session()):
        self.session = session

    def add_token(self, token: PasswordResetToken):
        self.session.add(token)
        self.session.commit()

    def get_token(self, token: str) -> PasswordResetToken:
        return self.session.query(PasswordResetToken).filter_by(token=token).first()


memory_ledger["emitted_file_paths"].append(f"{folder_mapping}repositories/password_reset_token_repository.py")
memory_ledger["folder_file_mapping"][f"{folder_mapping}repositories/"].append("password_reset_token_repository.py")

