# Epic Title: User Account Management System

from sqlalchemy.orm import Session
from backend.user_account_management.models.password_reset import PasswordReset

class PasswordResetRepository:
    def __init__(self, session: Session):
        self.session = session

    def add_password_reset(self, password_reset: PasswordReset) -> None:
        self.session.add(password_reset)
        self.session.commit()

    def get_password_reset(self, reset_token: str) -> PasswordReset:
        return self.session.query(PasswordReset).filter_by(reset_token=reset_token).first()

    def invalidate_password_reset(self, reset_token: str) -> None:
        password_reset = self.get_password_reset(reset_token)
        if password_reset:
            self.session.delete(password_reset)
            self.session.commit()


memory_ledger["emitted_file_paths"].append("backend/user_account_management/repositories/password_reset_repository.py")
memory_ledger["public_interfaces"].append("PasswordResetRepository")
memory_ledger["imports"].append("backend/user_account_management/models/password_reset:PasswordReset")
memory_ledger["folder_file_mapping"]["backend/user_account_management/repositories/"].append("password_reset_repository.py")

