# Epic Title: Product Catalog Management System

from sqlalchemy.orm import Session
from backend.product_catalog_management.models.category import Category

class CategoryRepository:
    def __init__(self, session: Session):
        self.session = session

    def add_category(self, category: Category) -> None:
        self.session.add(category)
        self.session.commit()

    def get_category_by_name(self, name: str) -> Category:
        return self.session.query(Category).filter_by(name=name).first()
    
    def get_all_categories(self) -> list:
        return self.session.query(Category).all()


memory_ledger["emitted_file_paths"].append("backend/product_catalog_management/repositories/category_repository.py")
memory_ledger["public_interfaces"].append("CategoryRepository")
memory_ledger["folder_file_mapping"]["backend/product_catalog_management/repositories/"].append("category_repository.py")

