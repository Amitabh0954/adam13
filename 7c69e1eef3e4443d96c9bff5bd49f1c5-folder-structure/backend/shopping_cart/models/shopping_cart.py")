# Epic Title: Shopping Cart Functionality

from sqlalchemy.orm import Session
from backend.shopping_cart.models.shopping_cart import ShoppingCart
from backend.shopping_cart.models.cart_item import CartItem

class ShoppingCartRepository:
    def __init__(self, session: Session):
        self.session = session

    def create_cart(self, shopping_cart: ShoppingCart) -> None:
        self.session.add(shopping_cart)
        self.session.commit()

    def add_item_to_cart(self, cart_item: CartItem) -> None:
        self.session.add(cart_item)
        self.session.commit()

    def get_cart_by_user_id(self, user_id: int) -> ShoppingCart:
        return self.session.query(ShoppingCart).filter_by(user_id=user_id).first()

    def get_cart_items_by_cart_id(self, cart_id: int) -> list:
        return self.session.query(CartItem).filter_by(cart_id=cart_id).all()


