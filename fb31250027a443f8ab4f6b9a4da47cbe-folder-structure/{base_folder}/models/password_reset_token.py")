# Epic Title: User Account Management

from sqlalchemy.orm import Session
from datetime import datetime
from backend.user_account_management.models.user import Session as db_session
from backend.user_account_management.models.password_reset_token import PasswordResetToken

class PasswordResetRepository:
    def __init__(self, session: Session = db_session()):
        self.session = session
    
    def add_password_reset_token(self, password_reset_token: PasswordResetToken):
        self.session.add(password_reset_token)
        self.session.commit()
    
    def get_password_reset_token(self, token: str) -> PasswordResetToken:
        return self.session.query(PasswordResetToken).filter_by(token=token).first()
    
    def delete_password_reset_token(self, password_reset_token: PasswordResetToken):
        self.session.delete(password_reset_token)
        self.session.commit()
    
    def delete_expired_tokens(self):
        self.session.query(PasswordResetToken).filter(PasswordResetToken.expires_at < datetime.utcnow()).delete()
        self.session.commit()


memory_ledger["emitted_file_paths"].append(f"{base_folder}/repositories/password_reset_repository.py")
memory_ledger["folder_file_mapping"][f"{base_folder}/repositories/"].append("password_reset_repository.py")

