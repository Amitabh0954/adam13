# Epic Title: User Account Management

from sqlalchemy import Column, Integer, String, ForeignKey
from sqlalchemy.orm import relationship
from ..models.user import Base, User

class UserSession(Base):
    __tablename__ = 'sessions'

    id = Column(Integer, primary_key=True, autoincrement=True)
    user_id = Column(Integer, ForeignKey('users.id'), nullable=False)
    token = Column(String(255), unique=True, nullable=False)
    
    user = relationship('User', backref='sessions')

    def __repr__(self):
        return f"<UserSession(user_id={self.user_id}, token={self.token})>"


memory_ledger["emitted_file_paths"].append(f"{base_folder}/session/user_session.py")
memory_ledger["folder_file_mapping"][f"{base_folder}/session/"].append("user_session.py")

