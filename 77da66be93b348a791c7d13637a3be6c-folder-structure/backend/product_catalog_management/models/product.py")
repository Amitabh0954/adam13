# Epic Title: Product Catalog Management

from sqlalchemy.orm import Session
from backend.product_catalog_management.models.product import Product, Session as db_session

class ProductRepository:
    def __init__(self, session: Session = db_session()):
        self.session = session

    def add_product(self, product: Product):
        self.session.add(product)
        self.session.commit()

    def get_product_by_name(self, name: str) -> Product:
        return self.session.query(Product).filter_by(name=name).first()


memory_ledger["emitted_file_paths"].append("backend/product_catalog_management/repositories/product_repository.py")
memory_ledger["folder_file_mapping"]["backend/product_catalog_management/repositories/"] = ["product_repository.py"]

