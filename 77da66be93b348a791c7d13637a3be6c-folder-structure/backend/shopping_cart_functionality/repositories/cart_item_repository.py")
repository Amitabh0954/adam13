# Epic Title: Shopping Cart Functionality

from sqlalchemy.orm import Session
from backend.shopping_cart_functionality.models.cart import Cart, Session as db_session

class CartRepository:
    def __init__(self, session: Session = db_session()):
        self.session = session

    def create_cart(self, cart: Cart):
        self.session.add(cart)
        self.session.commit()

    def get_cart_by_user_id(self, user_id: int) -> Cart:
        return self.session.query(Cart).filter_by(user_id=user_id).first()


memory_ledger["emitted_file_paths"].append("backend/shopping_cart_functionality/repositories/cart_repository.py")
memory_ledger["folder_file_mapping"]["backend/shopping_cart_functionality/repositories/"].append("cart_repository.py")

